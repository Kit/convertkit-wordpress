{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 1,
    "name": "convertkit/native-form",
    "title": "Kit Native Form",
    "category": "kit",
    "description": "Displays a Kit Native Form.",
    "keywords": [
        "form",
        "embed",
        "subscriber",
        "email",
        "convertkit"
    ],
    "textdomain": "convertkit",
    "attributes": {
        "display_name_field": {
            "type": "boolean"
        },
        "display_labels": {
            "type": "boolean"
        },
        "text": {
            "type": "string"
        },
        "fontSize": {
            "type": "string"
        },
        "style": {
            "type": "object"
        },
        "backgroundColor": {
            "type": "string"
        },
        "textColor": {
            "type": "string"
        },
        "is_gutenberg_example": {
            "type": "boolean",
            "default": false
        }
    },
    "supports": {
        "className": true,
        "color": {
            "background": true,
            "text": true
        },
        "typography": {
            "fontSize": true,
            "lineHeight": true
        },
        "spacing": {
            "margin": true,
            "padding": true
        }
    },
    "editorScript": "convertkit-gutenberg"
}